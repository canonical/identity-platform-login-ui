{
  "name": "identity-platform-login-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@canonical/react-components": "0.42.0",
    "@ory/client": "1.2.4",
    "@ory/integrations": "1.1.1",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-toastify": "9.1.3",
    "sass-embedded": "1.66.1",
    "vanilla-framework": "3.14.0"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "clean": "rm -rf node_modules css static/css *.log _site/ .next/",
    "dev": "npm run build-css && next dev",
    "build": "npm run build-css && next build && next export -o dist",
    "start": "next start",
    "lint": "next lint",
    "build-css": "node_modules/sass/sass.js --load-path node_modules --source-map static/sass:static/css && postcss --map false --use autoprefixer --replace 'static/css/**/*.css'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@next/next/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.7",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.2",
    "postcss": "8.4.24",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.8",
    "typescript": "5.2.2"
  }
}
